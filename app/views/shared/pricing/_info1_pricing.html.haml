.evenbg.main-plans
  .container
    .container.container-plans{"data-plan" => "yearly"}
      .inner-container.flex-container
        #starter.column
          %header.bg-blue
            %span.plan-name Cơ bản
            %span.plan-price
              %span.price> 125.000
              \/tháng
          %section
            %ul.feature-list
              %li.feature-list-item
                %span.item-icon
                  %img{:src => asset_path("records.svg")}/
                %span.item-title 1.000
                %span.item-subtitle Bản ghi
              %li.feature-list-item
                %span.item-icon
                  %img{:src => asset_path("operations.svg")}/
                %span.item-title 5
                %span.item-subtitle loại biểu đồ thống kê cơ bản
              %li.feature-list-item
                %span.item-icon
                  %img{:src => asset_path("support.svg")}/
                %span.item-title.small-title Hỗ trợ qua email
            %a.btn.btn-info{:href => "/signup"} Bắt đầu dùng thử miễn phí
        #growth.column
          %header.bg-green
            %span.plan-name Nâng cao
            %span.plan-price
              %span.price> 199.000
              \/tháng
          %section
            %ul.feature-list
              %li.feature-list-item
                %span.item-icon
                  %img{:src => asset_path("records.svg")}/
                %span.item-title 10.000
                %span.item-subtitle Bản ghi
              %li.feature-list-item
                %span.item-icon
                  %img{:src => asset_path("operations.svg")}/
                %span.item-title 10
                %span.item-subtitle loại biểu đồ thống kê và báo cáo
              %li.feature-list-item
                %span.item-icon
                  %img{:src => asset_path("support.svg")}/
                %span.item-title.small-title Hỗ trợ qua Email
              %li.feature-list-item
                %span.item-icon
                  %img{:src => asset_path("ux-guidance.svg")}/
                %span.item-title.small-title Hỗ trợ điện thoại
            %a.btn.btn-info{:href => "/signup"} Bắt đầu dùng thử miễn phí
        #pro.column
          %header.bg-deep-blue
            %span.plan-name Chuyên nghiệp
            %span.plan-price
              %span.price> 299.000
              \/tháng
          %section
            %ul.feature-list
              %li.feature-list-item
                %span.item-icon
                  %img{:src => asset_path("records.svg")}/
                %span.item-title 50.000
                %span.item-subtitle Bản ghi
              %li.feature-list-item
                %span.item-icon
                  %img{:src => asset_path("operations.svg")}/
                %span.item-title Toàn bộ
                %span.item-subtitle các biểu đồ và báo cáo thống kê
              %li.feature-list-item
                %span.item-icon
                  %img{:src => asset_path("support.svg")}/
                %span.item-title.small-title Hỗ trợ trực tuyến
              %li.feature-list-item
                %span.item-icon
                  %img{:src => asset_path("ux-guidance.svg")}/
                %span.item-title.small-title Hỗ trợ điện thoại
              %li.feature-list-item
                %span.item-icon
                  %img{:src => asset_path("support.svg")}/
                %span.item-title.small-title Hỗ trợ phát triển
              %li.feature-list-item
                %span.item-icon
                  %img{:src => asset_path("extensions-plugins.svg")}/
                %span.item-title.small-title.multiline Hệ thống mở rộng
            %a.btn.btn-info{:href => "/signup"} Bắt đầu dùng thử miễn phí
